namespace Sink.ApplicationDomain.Buffer;

table Mail {
  uid:string;
  folder:string;
  sender:string;
  senderName:string;
  subject:string;
  date:string;
  unread:bool = false;
  important:bool = false;
  mimeMessage:string;
  draft:bool = false;
  trash:bool = false;
  sent:bool = false;
  messageId:string;
  parentMessageId:string;
}

root_type Mail;
file_identifier "AKFB";
